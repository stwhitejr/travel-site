{
  "rewrites": [
    {
      "source": "/media/:path*",
      "destination": "https://pajqyzcpmfrhpupdtupn.supabase.co/storage/v1/object/public/:path*"
    }
  ],
  "headers": [
    {
      "source": "/media/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "x-vercel-enable-rewrite-caching",
          "value": "1"
        }
      ]
    }
  ]
}
